        "endTime": "2024-07-15T16:30:00.000Z",
        "productOverride": {
          "synopsis": {
            "longText": "Programma waarin we verslag doen van de Ronde van Frankrijk, gecombineerd met nieuws en actualiteiten.",
            "shortText": ""
          },
