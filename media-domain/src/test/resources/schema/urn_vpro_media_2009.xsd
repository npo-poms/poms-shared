<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" version="1.0" targetNamespace="urn:vpro:media:2009" xmlns="urn:vpro:media:2009" xmlns:shared="urn:vpro:shared:2009" xmlns:tns="urn:vpro:media:2009" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:import namespace="urn:vpro:shared:2009" schemaLocation="urn_vpro_shared_2009.xsd"/>

  <xs:import namespace="http://www.w3.org/XML/1998/namespace"/>

  <xs:element name="group" type="groupType"/>

  <xs:element name="mediaInformation" type="mediaTableType"/>

  <xs:element name="memberRef" type="memberRefType"/>

  <xs:element name="program" type="programType"/>

  <xs:element name="schedule" type="scheduleType"/>

  <xs:element name="segment" type="segmentType"/>

  <xs:element name="streamingStatus" type="streamingStatus"/>

  <xs:complexType name="programType" final="extension restriction">
    <xs:complexContent>
      <xs:extension base="baseMediaType">
        <xs:sequence>
          <xs:element name="scheduleEvents" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="scheduleEvent" type="scheduleEventType" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="episodeOf" type="memberRefType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="segments" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element ref="segment" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="poProgType" type="xs:string" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="type" type="programTypeEnum" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="baseMediaType" abstract="true">
    <xs:complexContent>
      <xs:extension base="shared:publishableObjectType">
        <xs:sequence>
          <xs:element name="crid" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="broadcaster" type="broadcasterType" maxOccurs="unbounded"/>
          <xs:element name="portal" type="portalType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="exclusive" type="portalRestrictionType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="region" type="geoRestrictionType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="title" type="titleType" maxOccurs="unbounded"/>
          <xs:element name="description" type="descriptionType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="genre" type="genreType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="tag" type="tagType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="intentions" type="intentionsType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="targetGroups" type="targetGroupsType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="geoLocations" type="geoLocationsType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="topics" type="topicsType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="source" type="xs:string" minOccurs="0"/>
          <xs:element name="country" type="countryType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="language" type="languageType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="isDubbed" type="xs:boolean" minOccurs="0"/>
          <xs:element name="availableSubtitles" type="availableSubtitlesType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="avAttributes" type="avAttributesType" minOccurs="0"/>
          <xs:element name="releaseYear" type="xs:short" minOccurs="0"/>
          <xs:element name="duration" type="shared:durationType" minOccurs="0"/>
          <xs:element name="credits" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                  <xs:element name="person" type="personType"/>
                  <xs:element name="name" type="nameType"/>
                </xs:choice>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="award" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="descendantOf" type="descendantRefType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="memberOf" type="memberRefType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="ageRating" type="ageRating" minOccurs="0"/>
          <xs:element name="contentRating" type="contentRatingEnum" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="email" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="website" type="websiteType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="twitter" type="twitterRefType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="teletext" type="xs:short" minOccurs="0"/>
          <xs:element name="prediction" type="predictionType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="locations" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="location" type="locationType" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="relation" type="relationType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="images" minOccurs="0">
            <xs:complexType>
              <xs:sequence>
                <xs:element ref="shared:image" minOccurs="0" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="adoptQualityFromPlus" type="xs:boolean"/>
        <xs:attribute name="avType" type="avTypeEnum" use="required"/>
        <xs:attribute name="embeddable" type="xs:boolean" use="required"/>
        <xs:attribute name="hasSubtitles" type="xs:boolean"/>
        <xs:attribute name="mergedTo" type="xs:string"/>
        <xs:attribute name="mid" type="xs:string" use="required"/>
        <xs:attribute name="sortDate" type="xs:dateTime" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="scheduleEventType">
    <xs:sequence>
      <xs:element name="title" type="scheduleEventTitle" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="description" type="scheduleEventDescription" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="repeat" type="repeatType" minOccurs="0"/>
      <xs:element name="memberOf" type="xs:string" minOccurs="0"/>
      <xs:element name="avAttributes" type="avAttributesType" minOccurs="0"/>
      <xs:element name="textSubtitles" type="xs:string" minOccurs="0"/>
      <xs:element name="textPage" type="xs:string" minOccurs="0"/>
      <xs:element name="guideDay" type="xs:date" minOccurs="0"/>
      <xs:element name="start" type="xs:dateTime" minOccurs="0"/>
      <xs:element name="offset" type="xs:duration" minOccurs="0"/>
      <xs:element name="duration" type="xs:duration" minOccurs="0"/>
      <xs:element name="poProgID" type="xs:string" minOccurs="0"/>
      <xs:element name="poSeriesID" type="xs:string" minOccurs="0"/>
      <xs:element name="primaryLifestyle" type="lifestyle" minOccurs="0"/>
      <xs:element name="secondaryLifestyle" type="secondaryLifestyle" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="channel" type="channelEnum"/>
    <xs:attribute name="imi" type="xs:string"/>
    <xs:attribute name="midRef" type="xs:string" use="required"/>
    <xs:attribute name="net" type="net"/>
    <xs:attribute name="type" type="scheduleEventTypeEnum"/>
    <xs:attribute name="urnRef" type="xs:string" use="required"/>
  </xs:complexType>

  <xs:complexType name="scheduleEventTitle">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="owner" type="shared:ownerTypeEnum" use="required"/>
        <xs:attribute name="type" type="textualTypeEnum" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="scheduleEventDescription">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="owner" type="shared:ownerTypeEnum" use="required"/>
        <xs:attribute name="type" type="textualTypeEnum" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="repeatType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="isRerun" type="xs:boolean" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="avAttributesType">
    <xs:sequence>
      <xs:element name="bitrate" type="xs:int" minOccurs="0"/>
      <xs:element name="byteSize" type="xs:long" minOccurs="0"/>
      <xs:element name="avFileFormat" type="avFileFormatEnum" minOccurs="0"/>
      <xs:element name="videoAttributes" type="videoAttributesType" minOccurs="0"/>
      <xs:element name="audioAttributes" type="audioAttributesType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="videoAttributesType">
    <xs:sequence>
      <xs:element name="color" type="colorType" minOccurs="0"/>
      <xs:element name="videoCoding" type="xs:string" minOccurs="0"/>
      <xs:element name="aspectRatio" type="aspectRatio" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="width" type="xs:int"/>
    <xs:attribute name="heigth" type="xs:int"/>
  </xs:complexType>

  <xs:complexType name="audioAttributesType">
    <xs:sequence>
      <xs:element name="numberOfChannels" type="xs:int" minOccurs="0"/>
      <xs:element name="audioCoding" type="xs:string" minOccurs="0"/>
      <xs:element name="language" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="lifestyle">
    <xs:restriction base="xs:string"/>
  </xs:simpleType>

  <xs:simpleType name="secondaryLifestyle">
    <xs:restriction base="xs:string"/>
  </xs:simpleType>

  <xs:simpleType name="net">
    <xs:restriction base="xs:string"/>
  </xs:simpleType>

  <xs:complexType name="memberRefType">
    <xs:sequence>
      <xs:element name="episodeOf" type="recursiveMemberRef" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="memberOf" type="recursiveMemberRef" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="segmentOf" type="recursiveMemberRef" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="added" type="xs:dateTime"/>
    <xs:attribute name="cridRef" type="xs:string"/>
    <xs:attribute name="highlighted" type="xs:boolean" use="required"/>
    <xs:attribute name="midRef" type="xs:string"/>
    <xs:attribute name="index" type="xs:nonNegativeInteger"/>
    <xs:attribute name="type" type="mediaTypeEnum"/>
    <xs:attribute name="urnRef" type="xs:string"/>
  </xs:complexType>

  <xs:complexType name="recursiveMemberRef">
    <xs:sequence>
      <xs:element name="memberOf" type="recursiveMemberRef" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="episodeOf" type="recursiveMemberRef" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="segmentOf" type="recursiveMemberRef" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="midRef" type="xs:string"/>
    <xs:attribute name="type" type="mediaTypeEnum"/>
    <xs:attribute name="index" type="xs:int"/>
    <xs:attribute name="highlighted" type="xs:boolean"/>
    <xs:attribute name="circular" type="xs:boolean"/>
  </xs:complexType>

  <xs:complexType name="segmentType" final="extension restriction">
    <xs:complexContent>
      <xs:extension base="baseMediaType">
        <xs:sequence>
          <xs:element name="segmentOf" type="recursiveMemberRef" minOccurs="0"/>
          <xs:element name="start" type="xs:duration"/>
        </xs:sequence>
        <xs:attribute name="midRef" type="xs:string" use="required"/>
        <xs:attribute name="type" type="segmentTypeEnum" use="required"/>
        <xs:attribute name="urnRef" type="xs:string" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="broadcasterType">
    <xs:simpleContent>
      <xs:extension base="organizationType"/>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="organizationType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="id" type="xs:string"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="portalType">
    <xs:simpleContent>
      <xs:extension base="organizationType"/>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="portalRestrictionType">
    <xs:complexContent>
      <xs:extension base="restriction">
        <xs:sequence/>
        <xs:attribute name="portalId" type="xs:string"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="restriction" abstract="true">
    <xs:sequence/>
    <xs:attribute name="start" type="xs:dateTime"/>
    <xs:attribute name="stop" type="xs:dateTime"/>
  </xs:complexType>

  <xs:complexType name="geoRestrictionType">
    <xs:complexContent>
      <xs:extension base="restriction">
        <xs:sequence/>
        <xs:attribute name="regionId" type="region"/>
        <xs:attribute name="platform" type="platformTypeEnum"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="titleType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="owner" type="shared:ownerTypeEnum" use="required"/>
        <xs:attribute name="type" type="textualTypeEnum" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="descriptionType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="owner" type="shared:ownerTypeEnum" use="required"/>
        <xs:attribute name="type" type="textualTypeEnum" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="genreType">
    <xs:sequence>
      <xs:element name="term" type="termType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:string"/>
  </xs:complexType>

  <xs:simpleType name="termType">
    <xs:restriction base="xs:string"/>
  </xs:simpleType>

  <xs:complexType name="tagType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute ref="xml:lang"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="intentionsType">
    <xs:sequence>
      <xs:element name="intention" type="intentionType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="owner" type="shared:ownerTypeEnum"/>
  </xs:complexType>

  <xs:simpleType name="intentionType">
    <xs:restriction base="intentionEnum"/>
  </xs:simpleType>

  <xs:complexType name="targetGroupsType">
    <xs:sequence>
      <xs:element name="targetGroup" type="targetGroupType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="owner" type="shared:ownerTypeEnum"/>
  </xs:complexType>

  <xs:simpleType name="targetGroupType">
    <xs:restriction base="targetGroupEnum"/>
  </xs:simpleType>

  <xs:complexType name="geoLocationsType">
    <xs:sequence>
      <xs:element name="geoLocation" type="geoLocationType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="owner" type="shared:ownerTypeEnum"/>
  </xs:complexType>

  <xs:complexType name="geoLocationType">
    <xs:sequence>
      <xs:element name="name" type="xs:string" minOccurs="0"/>
      <xs:element name="scopeNote" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="gtaaUri" type="xs:string"/>
    <xs:attribute name="gtaaStatus" type="gtaaStatusType"/>
    <xs:attribute name="role" type="geoRoleType" use="required"/>
  </xs:complexType>

  <xs:complexType name="topicsType">
    <xs:sequence>
      <xs:element name="topic" type="topicType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="owner" type="shared:ownerTypeEnum"/>
  </xs:complexType>

  <xs:complexType name="topicType">
    <xs:sequence>
      <xs:element name="name" type="xs:string" minOccurs="0"/>
      <xs:element name="scopeNote" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="gtaaUri" type="xs:string"/>
    <xs:attribute name="gtaaStatus" type="gtaaStatusType"/>
  </xs:complexType>

  <xs:complexType name="countryType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="code" type="xs:string"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="languageType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="code" type="xs:string"/>
        <xs:attribute name="usage" type="languageUsageEnum"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="availableSubtitlesType">
    <xs:sequence/>
    <xs:attribute name="language" type="xs:string"/>
    <xs:attribute name="type" type="shared:subtitlesTypeEnum"/>
    <xs:attribute name="workflow" type="shared:subtitlesWorkflowEnum"/>
  </xs:complexType>

  <xs:complexType name="personType">
    <xs:complexContent>
      <xs:extension base="creditsType">
        <xs:sequence>
          <xs:element name="givenName" type="xs:string" minOccurs="0"/>
          <xs:element name="familyName" type="xs:string" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="gtaaUri" type="xs:string"/>
        <xs:attribute name="gtaaStatus" type="gtaaStatusType"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="creditsType" abstract="true">
    <xs:sequence/>
    <xs:attribute name="role" type="roleType" use="required"/>
  </xs:complexType>

  <xs:complexType name="nameType">
    <xs:complexContent>
      <xs:extension base="creditsType">
        <xs:sequence>
          <xs:element name="name" type="xs:string" minOccurs="0"/>
          <xs:element name="scopeNote" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="gtaaUri" type="xs:string"/>
        <xs:attribute name="gtaaStatus" type="gtaaStatusType"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="descendantRefType">
    <xs:sequence/>
    <xs:attribute name="urnRef" type="xs:string"/>
    <xs:attribute name="midRef" type="xs:string"/>
    <xs:attribute name="type" type="mediaTypeEnum"/>
  </xs:complexType>

  <xs:simpleType name="websiteType">
    <xs:restriction base="xs:string"/>
  </xs:simpleType>

  <xs:complexType name="twitterRefType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="type" type="type"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="predictionType">
    <xs:simpleContent>
      <xs:extension base="platformTypeEnum">
        <xs:attribute name="state" type="predictionStateEnum"/>
        <xs:attribute name="publishStart" type="xs:dateTime"/>
        <xs:attribute name="publishStop" type="xs:dateTime"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="locationType">
    <xs:complexContent>
      <xs:extension base="shared:publishableObjectType">
        <xs:sequence>
          <xs:element name="programUrl" type="xs:string" minOccurs="0"/>
          <xs:element name="avAttributes" type="avAttributesType" minOccurs="0"/>
          <xs:element name="subtitles" type="xs:string" minOccurs="0"/>
          <xs:element name="offset" type="xs:duration" minOccurs="0"/>
          <xs:element name="duration" type="xs:duration" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="owner" type="shared:ownerTypeEnum" use="required"/>
        <xs:attribute name="platform" type="platformTypeEnum"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="relationType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="uriRef" type="xs:string"/>
        <xs:attribute name="broadcaster" type="xs:string" use="required"/>
        <xs:attribute name="type" type="xs:string" use="required"/>
        <xs:attribute name="urn" type="xs:string"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="groupType" final="extension restriction">
    <xs:complexContent>
      <xs:extension base="baseMediaType">
        <xs:sequence>
          <xs:element name="poSequenceInformation" type="xs:string" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="isOrdered" type="xs:boolean" use="required"/>
        <xs:attribute name="type" type="groupTypeEnum" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="scheduleType">
    <xs:sequence>
      <xs:element name="scheduleEvent" type="scheduleEventType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="channel" type="channelEnum"/>
    <xs:attribute name="net" type="net"/>
    <xs:attribute name="start" type="xs:dateTime"/>
    <xs:attribute name="releaseVersion" type="xs:int"/>
    <xs:attribute name="reruns" type="xs:boolean"/>
    <xs:attribute name="stop" type="xs:dateTime"/>
  </xs:complexType>

  <xs:complexType name="mediaTableType">
    <xs:sequence>
      <xs:element name="programTable" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="program" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="groupTable" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="group" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="locationTable" type="locationTableType" minOccurs="0"/>
      <xs:element ref="schedule" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="publicationTime" type="xs:dateTime"/>
    <xs:attribute name="publisher" type="xs:string"/>
    <xs:attribute name="version" type="xs:short"/>
    <xs:attribute name="source" type="xs:string"/>
  </xs:complexType>

  <xs:complexType name="locationTableType">
    <xs:sequence>
      <xs:element name="location" type="locationType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="scheduleEvent" type="scheduleEventType" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="streamingStatus">
    <xs:sequence/>
    <xs:attribute name="withDrm" type="streamingStatusValue"/>
    <xs:attribute name="withoutDrm" type="streamingStatusValue"/>
    <xs:attribute name="audioWithoutDrm" type="streamingStatusValue"/>
  </xs:complexType>

  <xs:complexType name="standaloneMemberRefType">
    <xs:sequence/>
    <xs:attribute name="added" type="xs:dateTime"/>
    <xs:attribute name="childRef" type="xs:string"/>
    <xs:attribute name="highlighted" type="xs:boolean"/>
    <xs:attribute name="index" type="xs:int"/>
    <xs:attribute name="midRef" type="xs:string"/>
    <xs:attribute name="objectType" type="objectType"/>
    <xs:attribute name="owner" type="shared:ownerTypeEnum"/>
    <xs:attribute name="type" type="mediaTypeEnum"/>
  </xs:complexType>

  <xs:simpleType name="textualTypeEnum">
    <xs:restriction base="xs:string">
      <xs:enumeration value="MAIN"/>
      <xs:enumeration value="LONG"/>
      <xs:enumeration value="SHORT"/>
      <xs:enumeration value="SUB"/>
      <xs:enumeration value="EPISODE"/>
      <xs:enumeration value="ORIGINAL"/>
      <xs:enumeration value="KICKER"/>
