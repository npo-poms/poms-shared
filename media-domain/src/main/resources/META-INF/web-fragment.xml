<web-fragment
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xsi:schemaLocation="
              http://java.sun.com/xml/ns/javaee https://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd"
              id="media-domain-kijkwijzer" version="3.0">
  <name>media_domain_kijkwijzer</name>
  <distributable />
  <servlet>
    <servlet-name>kijkwijzericons</servlet-name>
    <jsp-file>/kijkwijzer/icons.svg.jspx</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>kijkwijzericons</servlet-name>
    <url-pattern>/kijkwijzer/icons/</url-pattern>
  </servlet-mapping>
</web-fragment>
